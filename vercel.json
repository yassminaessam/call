{
  "version": 2,
  "buildCommand": "npm run build",
  "outputDirectory": "dist/spa",
  "installCommand": "npm install",
  "framework": null,
  "routes": [
    { 
      "handle": "filesystem" 
    },
    { 
      "src": "/(.*)", 
      "dest": "/index.html" 
    }
  ],
  "env": {
    "DATABASE_URL": "@database_url",
    "GRANDSTREAM_HOST": "@grandstream_host",
    "GRANDSTREAM_PORT": "@grandstream_port", 
    "GRANDSTREAM_USERNAME": "@grandstream_username",
    "GRANDSTREAM_PASSWORD": "@grandstream_password",
    "GRANDSTREAM_API_VERSION": "@grandstream_api_version",
    "UCM_MODE": "@ucm_mode"
  }
}
