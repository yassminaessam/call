# Grandstream UCM6304A PBX Configuration
# PRODUCTION MODE: Replace these with your actual UCM6304A settings
# Find these settings in your UCM6304A web interface:
# - Host: Network Settings > Network Settings > LAN Settings
# - Port: Network Settings > Provisioning > Config Server
# - Username/Password: System Settings > Administrator > Accounts

GRANDSTREAM_HOST=YOUR_UCM_IP_ADDRESS_HERE    # e.g., 192.168.1.20
GRANDSTREAM_PORT=8088                        # Default UCM API port
GRANDSTREAM_USERNAME=YOUR_UCM_USERNAME       # Your UCM admin username
GRANDSTREAM_PASSWORD=YOUR_UCM_PASSWORD       # Your UCM admin password
GRANDSTREAM_API_VERSION=v1.0

# Production/Demo Mode Detection
UCM_MODE=production                          # Set to 'production' when using real UCM
UCM_CONNECTION_TIMEOUT=10000                 # Connection timeout in milliseconds

# CDR Sync Configuration  
CDR_SYNC_INTERVAL=300000                     # Automatic sync interval in milliseconds (5 minutes)
CDR_REALTIME_ENABLED=false                   # Enable when UCM webhook is configured
CDR_MAX_RECORDS_PER_SYNC=1000               # Maximum records to sync per batch

# CDR Connection Configuration
# Used for automatic configuration of the CDR connector form
CDR_MODE=HTTPS                               # Connection mode: HTTPS or TCP
CDR_ACTIVE=true                              # Enable/disable CDR connector

# HTTPS Mode Settings
CDR_HTTPS_WEBHOOK_URL=                       # Your webhook endpoint URL (e.g., https://your-domain.com/api/cdr/receive)
CDR_HTTPS_AUTH_USERNAME=                     # Basic auth username for webhook
CDR_HTTPS_AUTH_PASSWORD=                     # Basic auth password for webhook
CDR_HTTPS_ENDPOINT_PATH=/api/cdr/receive     # Endpoint path for receiving CDR data

# TCP Mode Settings  
CDR_TCP_PORT=10001                           # TCP server port for receiving CDR data
CDR_TCP_BUFFER_SIZE=8192                     # TCP buffer size in bytes

# Common CDR Settings
CDR_ALLOWED_IPS=                             # Comma-separated list of allowed IP addresses (empty = allow all)
CDR_RETRY_ATTEMPTS=3                         # Number of retry attempts for failed operations
CDR_TIMEOUT=5000                             # Timeout in milliseconds
